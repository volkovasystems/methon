    "babel-runtime": "^6.23.0",
    "een": "^0.18.0",
    "eqe": "^0.2.0",
    "fname": "^0.8.0",
    "meton": "^0.1.0",
    "posp": "^0.6.0",
    "protype": "^0.21.0",
    "pyck": "^0.32.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
